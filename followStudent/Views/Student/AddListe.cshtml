@{
}
@model List<StudentModel>
<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Okul Numarası</th>
            <th>Adı</th>
            <th>Soyadı</th>
            <th>Sınıfı</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Surname</td>
                <td>@student.classes</td>
            </tr>
        }

    </tbody>
</table>